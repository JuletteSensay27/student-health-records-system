<Window x:Name="mainLoginWindow" x:Class="student_health_records_system.LogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:student_health_records_system"
        mc:Ignorable="d"
        Title="LogInWindow" Height="253" Width="358">
    <Grid>
        <TextBox x:Name="adminUNameTbx" HorizontalAlignment="Left" Margin="141,71,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="174"/>
        <Label x:Name="adminUNameLbl" Content="Admin Username" HorizontalAlignment="Left" Margin="26,67,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="adminPWordPbx" HorizontalAlignment="Left" Margin="141,119,0,0" VerticalAlignment="Top" Width="174"/>
        <Label x:Name="adminPWordLbl" Content="Admin Password" HorizontalAlignment="Left" Margin="26,115,0,0" VerticalAlignment="Top"/>
        <Button x:Name="loginBtn" Content="Log In" HorizontalAlignment="Left" Margin="228,184,0,0" VerticalAlignment="Top" Width="87" Click="loginBtn_Click"/>

    </Grid>
</Window>
